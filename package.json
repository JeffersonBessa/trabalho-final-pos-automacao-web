{
  "name": "trabalho-final-pos-automacao-web",
  "version": "1.0.0",
  "description": "Projeto Final para a disciplina de Automação de Testes de Software para a Camada de Interface",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test": "cypress run",
    "test:chrome": "cypress run --browser chrome",
    "test:firefox": "cypress run --browser firefox",
    "test:edge": "cypress run --browser edge",
    "report:merge": "mochawesome-merge cypress/reports/mochawesome/*.json > cypress/reports/mochawesome/report.json",
    "report:generate": "marge cypress/reports/mochawesome/report.json -f report -o cypress/reports/mochawesome",
    "report:clean": "rm -rf cypress/reports/mochawesome/*.json cypress/reports/mochawesome/*.html",
    "test:report": "npm run report:clean && npm run test && npm run report:merge && npm run report:generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JeffersonBessa/trabalho-final-pos-automacao-web.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JeffersonBessa/trabalho-final-pos-automacao-web/issues"
  },
  "homepage": "https://github.com/JeffersonBessa/trabalho-final-pos-automacao-web#readme",
  "devDependencies": {
    "cypress": "^15.5.0",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.3.0"
  }
}
